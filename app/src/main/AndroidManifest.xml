<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eveliinaheino.peippo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/customized_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/customized_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Peippo">
        <activity
            android:name=".ActivityManual"
            android:theme="@style/Theme.AppCompat.PopUp"
            android:exported="false" />
        <activity
            android:name=".ActivityTipDetails"
            android:parentActivityName=".ActivityTipList"
            android:exported="false" />
        <activity
            android:name=".ActivityFeedbackAfterSleep"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityFeedbackBeforeSleep"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityTipList"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityAddDataAfterSleep"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityAddDataBeforeSleep"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ActivitySeeData"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityNoDataSaved"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>